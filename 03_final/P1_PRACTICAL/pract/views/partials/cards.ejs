
<div class="card">
  <form action="/createfav">
    <div class="container">
        <div class="row justify-content-md-center">
            <h2 name="name"> <%= data.name   %></h2>
        </div>
        <div class="row">
              <p name="description"> <%= data.description %></p>
        </div>
        <div class="row justify-content-md-center">
              <img name="img" src="<%= data.image_url %> " /></br>
        </div>
        <div class="row justify-content-md-center">
              <button type="submit" class="btn btn-primary" > Add to Fav </button>
        </div>
    </div>
  </form>
</div>