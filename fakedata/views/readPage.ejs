<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Random Data App</h1>
    <ul>
        <li><a href="createPage">create data</a></li>
        <li><a href="readPage">read data</a></li>
    </ul>
    <table>
        <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        </tr>
       
        <%data.forEach(function(el) {%>
            <tr>
            <td><%= el.fName %> </td>
            <td> <%= el.lName %> </td>
            <td>  <%= el.phone %>  </td>
            </tr> 
              <% }); %> 
         
    </table>


</body>
</html>